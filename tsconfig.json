{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "target": "es5",
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "noEmit": true,
    "pretty": true,
    "lib": [
      "dom",
      "es6"
    ],
    "suppressImplicitAnyIndexErrors": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
