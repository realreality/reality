{
  "name": "Real Reality",
  "version": "VERSION_FROM_PACKAGE_JSON",
  "manifest_version": 2,
  "description": "DESCRIPTION_FROM_PACKAGE_JSON",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*",
    "pageCapture",
    "webRequest",
    "*://*.google.com/",
    "*://*.googleapis.com/"
  ],
  "web_accessible_resources": [
    "panel.html",
    "css/panel.css",
    "css/font-awesome.css",
    "fonts/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.bezrealitky.cz/*",
        "*://*.sreality.cz/*",
        "*://*.maxirealitypraha.cz/*",
        "*://reality.idnes.cz/*"
      ],
      "js": [
        "vendor.bundle.js",
        "contentscript.bundle.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19-off.png",
      "38": "images/icon-38-off.png"
    },
    "default_title": "Real Reality",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "vendor.bundle.js",
      "background.bundle.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://maps.googleapis.com https://www.google-analytics.com; object-src 'self'"
}
